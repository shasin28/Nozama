  int maxProfit(int K, int N, int A[]) {
        int dp[K+1][N];
        for(int i=0;i<=K;i++)
        dp[i][0]=0;
        for(int i=1;i<N;i++)
        dp[0][i]=0;
        for(int i=1;i<=K;i++)
        {
            int maxDiff=INT_MIN;
            for(int j=1;j<N;j++)
            {
                maxDiff=max(maxDiff,dp[i-1][j-1]-A[j-1]);
                dp[i][j]=max(dp[i][j-1],maxDiff+A[j]);
            }
        }
        return dp[K][N-1];
    }

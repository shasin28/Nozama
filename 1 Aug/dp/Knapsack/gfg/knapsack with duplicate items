class Solution{
public:
    int knapSack(int N, int W, int val[], int wt[])
    {
        int dp[W+1];
        memset(dp,0,sizeof(dp));
      
        for(int i=0;i<W+1;i++)
        {
        
        for(int j=0;j<N;j++)
        {
            if(i>=wt[j])
            dp[i]=max(dp[i-wt[j]]+val[j],dp[i]);
        }
        }
        return dp[W];
        
    }
    };

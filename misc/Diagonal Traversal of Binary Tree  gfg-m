vector<int> diagonal(Node *root)
{
   queue<Node*>q;
   q.push(root);
   vector<int>ans;
while(!q.empty())
{
    Node *temp=q.front();
    q.pop();
    while(temp)
    {
        ans.push_back(temp->data);
        if(temp->left)
        q.push(temp->left);
        //if(temp->right)
        temp=temp->right;
    }
}
return ans;
}

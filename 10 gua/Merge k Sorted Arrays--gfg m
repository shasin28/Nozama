//Initial Template for C++

#include<bits/stdc++.h>
#define N 105
using namespace std;
void printArray(vector<int> arr, int size)
{
for (int i=0; i < size; i++)
	cout << arr[i] << " ";
}


 // } Driver Code Ends
//User function Template for C++
class my
{
    public:
    int val,ii,jj;
    my(int a,int b,int c)
    {
        val=a;
        ii=b;
        jj=c;
    }
   
};
class myComp
{
    public:
     bool operator()(my m1,my m2)
    {
        return m1.val>m2.val;
    }
};

class Solution
{
    public:
    //Function to merge k sorted arrays.
    vector<int> mergeKArrays(vector<vector<int>> arr, int K)
    {
        priority_queue<my,vector<my>,myComp>pq;
    
        for(int i=0;i<K;i++)
        {
            my mm(arr[i][0],i,0);
            pq.push(mm);
        }
        vector<int>result(K*K);
        int j=0;
        while(!pq.empty())
        {
            my temp=pq.top();
            pq.pop();
            result[j]=temp.val;
            if(temp.jj+1<K)
            {
                my mm(arr[temp.ii][temp.jj+1],temp.ii,temp.jj+1);
                pq.push(mm);
            }
            j++;
        }
        return result;
    }
};

// { Driver Code Starts.

int main()
{
	int t;
	cin>>t;
	while(t--){
	    int k;
	    cin>>k;
	    vector<vector<int>> arr(N, vector<int> (N, 0));
	    for(int i=0; i<k; i++){
	        for(int j=0; j<k; j++)
	        {
	            cin>>arr[i][j];
	        }
	    }
	    Solution obj;
    	vector<int> output = obj.mergeKArrays(arr, k);
    	printArray(output, k*k);
    	cout<<endl;
    }
	return 0;
}





  // } Driver Code Ends
